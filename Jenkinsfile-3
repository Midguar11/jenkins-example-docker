pipeline {
  agent { dockerfile true }
  stages {
    stage('Test') {
      steps {
        sh '''
          node --version
          git --version
          curl --version
        '''
      }
    
     stage('Test Docker Slave'){
            agent { 
                label 'DockerAgent01'
            }
            steps{
                sh 'docker -v'
            }
        }
      
      
    }
  }
}
